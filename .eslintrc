{
  "extends": "airbnb/base",
  "rules": {
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "object-property-newline": 0,
    "comma-dangle": 0,
    "class-methods-use-this": 0,
    "object-curly-spacing": 0,
    "no-underscore-dangle": 0,
    "object-shorthand": 0
  },
  "env": {
    "browser": 1
  },
  "globals": {
    "angular": 1,
    "$": 1,
    "express": 1,
    "_": 1,
    "$$DEBUG": 1,
    "cc": 1,
    "NProgress":1
  },
  "parser": "babel-eslint"
}
